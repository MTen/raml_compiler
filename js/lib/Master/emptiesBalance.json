{
    "type": "object",
    "$schema": "http://json-schema.org/schema",
    "properties": {
        "emptiesBalanceID": {
            "type": "string"
        },
        "balance": {
            "type": "number",
            "description": "The balance that must be paid if the requested empties are not returned."
        },
        "orderItems": {
            "item": {
                "type": "array",
                "items": {
                    "$ref": "orderItem.json"
                }
            }
        },
        "required": [
            "emptiesID",
            "orderItems",
            "balance"
        ]
    }
}