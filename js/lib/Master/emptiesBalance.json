{
    "type": "object",
    "$schema": "http://json-schema.org/schema",
    "properties": {
        "emptiesBalanceID": {
            "type": "string"
        },
        "balance": {
            "type": "number",
            "description": "The balance that must be paid if the requested empties are not returned."
        },
        "empties": {
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "emptiesID": {
                        "type": "string"
                    },
                    "name": {
                        "type": "string"
                    },
                    "itemCount": {
                        "type": "integer"
                    }
                }
            }
        },
        "required": [
            "emptiesBalanceID",
            "balance",
            "empties",
        ]
    }
}