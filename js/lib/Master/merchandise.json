{
  "type": "object",
  "$schema": "http://json-schema.org/schema",
  "description": "A non-beverage product that may be purchased. For instance, an umbrella.",
  "properties": {
    "merchandiseID": {
      "type": "string"
    },
    "name": {
      "type": "string"
    },
    "description": {
      "type": "string"
    },
    "shippingInfo": {
      "type": "string"
    },
    "imageURL": {
      "type": "string"
    },
    "merchandiseTypeID": {
      "type": "string"
    },
    "family": {
      "type": "string",
      "description": "The brand family the merchandise is associated with."
    },
    "romanceCopy": {
      "type": "string"
    },
    "price": {
      "$ref": "price.json"
    }
  },
  "required": [
    "merchandiseID",
    "name"
  ]
}